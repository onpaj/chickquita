{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "validate": "npm run lint && npx tsc -b",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:smoke": "playwright test --grep @smoke",
    "test:e2e:full": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "test:crossbrowser": "playwright test --config=playwright.crossbrowser.config.ts",
    "test:crossbrowser:ui": "playwright test --config=playwright.crossbrowser.config.ts --ui",
    "test:crossbrowser:headed": "playwright test --config=playwright.crossbrowser.config.ts --headed",
    "test:crossbrowser:update": "playwright test --config=playwright.crossbrowser.config.ts --update-snapshots",
    "test:crossbrowser:report": "playwright show-report playwright-report/crossbrowser",
    "test:crossbrowser:chrome": "playwright test --config=playwright.crossbrowser.config.ts --project=chrome*",
    "test:crossbrowser:firefox": "playwright test --config=playwright.crossbrowser.config.ts --project=firefox*",
    "test:crossbrowser:safari": "playwright test --config=playwright.crossbrowser.config.ts --project=safari*",
    "test:crossbrowser:mobile": "playwright test --config=playwright.crossbrowser.config.ts --project=mobile*",
    "test:crossbrowser:breakpoints": "playwright test --config=playwright.crossbrowser.config.ts --project=breakpoint*",
    "generate:icons": "node scripts/generate-icons.mjs"
  },
  "dependencies": {
    "@clerk/clerk-react": "^5.60.0",
    "@clerk/localizations": "^3.35.3",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@hookform/resolvers": "^5.2.2",
    "@mui/icons-material": "^7.3.7",
    "@mui/lab": "^7.0.1-beta.21",
    "@mui/material": "^7.3.7",
    "@mui/x-date-pickers": "^8.27.0",
    "@tanstack/react-query": "^5.90.20",
    "axios": "^1.13.4",
    "date-fns": "^4.1.0",
    "dayjs": "^1.11.19",
    "dexie": "^4.3.0",
    "dompurify": "^3.3.1",
    "i18next": "^25.8.4",
    "i18next-browser-languagedetector": "^8.2.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-hook-form": "^7.71.1",
    "react-i18next": "^16.5.4",
    "react-router-dom": "^7.13.0",
    "recharts": "^3.7.0",
    "vite-plugin-pwa": "^1.2.0",
    "workbox-window": "^7.4.0",
    "zod": "^4.3.6",
    "zustand": "^5.0.11"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.11.1",
    "@clerk/testing": "^1.13.36",
    "@eslint/js": "^9.39.1",
    "@playwright/test": "^1.58.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/dompurify": "^3.0.5",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/coverage-v8": "^4.0.18",
    "@vitest/ui": "^4.0.18",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "jsdom": "^28.0.0",
    "sharp": "^0.34.5",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4",
    "vite-plugin-svgr": "^4.5.0",
    "vitest": "^4.0.18"
  }
}
