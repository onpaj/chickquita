# Backend Environment Variables - Example
# Copy this file to .env and fill in actual values
# NEVER commit .env file to git

# Database Connection
# Format: postgresql://username:password@endpoint/database?sslmode=require
ConnectionStrings__DefaultConnection=postgresql://username:password@endpoint/chickquita?sslmode=require

# Clerk Authentication (to be configured in US-015)
Clerk__SecretKey=
Clerk__Domain=
Clerk__WebhookSecret=whsec_your_webhook_secret_here

# Application Insights (Azure monitoring)
ApplicationInsights__InstrumentationKey=

# Logging
Logging__LogLevel__Default=Information
Logging__LogLevel__Microsoft.AspNetCore=Warning
